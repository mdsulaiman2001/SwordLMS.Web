@model IEnumerable<SwordLMS.Web.Models.Course>
@{

}
@foreach (var item in Model)
{
    <div class="card" style="width: 18rem;">
        <img src="@item.DisplayImagePath" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@item.Name</h5>
        </div>
    </div>
}
